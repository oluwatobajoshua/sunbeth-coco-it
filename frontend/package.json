  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "vercel-build": "CI=false react-scripts build",
  "build:no-lint": "node scripts/build-no-lint.js",
    "now-build": "npm run build:no-lint",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:test": "concurrently -k -s first -n emu,app \"firebase emulators:start --only firestore,auth,storage --project demo-sunbeth\" \"react-scripts start\"",
    "seed:demo": "node scripts/seed-demo.js 50",
    "seed:live": "node scripts/seed-live.js",
    "bridge:start": "node scripts/msal-bridge-server.js",
    "promote:user": "node scripts/promote-user.js",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed --project=chromium",
    "e2e:report": "playwright show-report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "concurrently": "^9.0.1"
